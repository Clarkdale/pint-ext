{
    "manifest_version": 2,
  
    "name": "Pint Sized",
    "description": "Make lengthy URLs pint sized easily without having to leave the page",
    "version": "0.1.2",
    "icons": { 
        "16": "icon.png",
        "48": "icon.png",
       "128": "icon.png" 
    },
    "browser_action": {
        "default_icon": "icon.png",
        "default_popup": "popup.html"
    },
    "permissions": [
        "activeTab"
    ]
}